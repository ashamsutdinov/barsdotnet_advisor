@model IEnumerable<UI.Models.ProductPhotoModel>
   @*неясность*@
@{
    ViewBag.Title = "Фотографии товара";
}

<h2>Фотографии товара</h2>
    <p>
    @Html.ActionLink("Добавить изображение", "Add")
</p>
<table>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.Raw("<img style='width:80px; height:60px;' src=\"data:image/jpeg;base64," 
                                + Convert.ToBase64String(item.Photo) + "\" />")
        </td>
    </tr>
}
 
</table>
